{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "resolvePackageJsonExports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@lib/config-tree": [
        "libs/config-tree/src"
      ],
      "@lib/config-tree/*": [
        "libs/config-tree/src/*"
      ],
      "@lib/config-validate": [
        "libs/config-validate/src"
      ],
      "@lib/config-validate/*": [
        "libs/config-validate/src/*"
      ],
      "@lib/minio": [
        "libs/minio/src"
      ],
      "@lib/minio-events-serializer": [
        "libs/minio-events-serializer/src"
      ],
      "@lib/minio-events-serializer/*": [
        "libs/minio-events-serializer/src/*"
      ],
      "@lib/minio/*": [
        "libs/minio/src/*"
      ],
      "@lib/redis-events": [
        "libs/redis-events/src"
      ],
      "@lib/redis-events/*": [
        "libs/redis-events/src/*"
      ],
      "@lib/sizes": [
        "libs/sizes/src"
      ],
      "@lib/sizes/*": [
        "libs/sizes/src/*"
      ]
    }
  }
}